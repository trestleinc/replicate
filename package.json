{
  "name": "@trestleinc/replicate",
  "version": "1.1.2",
  "description": "Offline-first data replication with Yjs CRDTs and Convex",
  "repository": "github:trestleinc/replicate",
  "homepage": "https://github.com/trestleinc/replicate#readme",
  "bugs": {
    "url": "https://github.com/trestleinc/replicate/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "convex",
    "replicate",
    "crdt",
    "yjs",
    "offline-first",
    "tanstack"
  ],
  "type": "module",
  "private": false,
  "resolutions": {
    "convex": "1.31.2",
    "@standard-schema/spec": "1.1.0",
    "zod": "4.2.1"
  },
  "overrides": {
    "convex": "1.31.2",
    "@standard-schema/spec": "1.1.0",
    "zod": "4.2.1"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/shared/index.d.ts",
        "default": "./dist/shared/index.js"
      }
    },
    "./package.json": "./package.json",
    "./client": {
      "import": {
        "@convex-dev/component-source": "./src/client/index.ts",
        "types": "./dist/client/index.d.ts",
        "default": "./dist/client/index.js"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      }
    },
    "./convex.config": {
      "import": {
        "@convex-dev/component-source": "./src/component/convex.config.ts",
        "types": "./dist/component/convex.config.d.ts",
        "default": "./dist/component/convex.config.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@effect/platform": "0.94.0",
    "@logtape/logtape": "1.3.5",
    "effect": "3.19.13"
  },
  "peerDependencies": {
    "@standard-schema/spec": "^1.1.0",
    "@tanstack/db": "^0.5.15",
    "convex": "^1.31.0",
    "lib0": "^0.2.0",
    "y-protocols": "^1.0.7",
    "yjs": "^13.6.0"
  },
  "peerDependenciesMeta": {
    "sql.js": {
      "optional": true
    },
    "@op-engineering/op-sqlite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "1.1.1",
    "@edge-runtime/vm": "5.0.0",
    "@microsoft/api-extractor": "7.55.2",
    "@standard-schema/spec": "1.1.0",
    "@stylistic/eslint-plugin": "5.6.1",
    "@tanstack/db": "0.5.15",
    "@types/node": "25.0.3",
    "@types/sql.js": "1.4.9",
    "convex": "1.31.2",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "fake-indexeddb": "6.2.5",
    "globals": "16.5.0",
    "lib0": "0.2.116",
    "publint": "0.3.16",
    "tsdown": "0.18.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1",
    "y-protocols": "1.0.7",
    "yjs": "13.6.28",
    "zod": "4.2.1"
  },
  "main": "./dist/shared/index.js",
  "types": "./dist/shared/index.d.ts",
  "module": "./dist/shared/index.js",
  "publishConfig": {
    "access": "public"
  }
}
